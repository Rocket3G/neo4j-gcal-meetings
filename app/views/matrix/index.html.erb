
<table class="table table-bordered table-striped table-hover table-condensed">

<thead>

<tr>
<th></th>
<% @matrix.each do |key, user| %>
  <th><%=key%></th>
<% end %>
</tr>
</thead>

<tbody>

<% @matrix.each do |key, user| %>

<tr>
<td class="header"><%=key%></td>

<% user.each do |k, val| %>
<td <% if k == key %> class="warning" <% end %>><%=val%></td>
<% end %>

</tr>
<% end %>

</tbody>

<tfoot>

</tfoot>

</table>
